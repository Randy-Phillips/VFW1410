//alert("gallery");

var currentWindow = Ti.UI.currentWindow;

var imageBlock = Ti.UI.createView({
	backgroundColor: "303030",
	top:150,
	right:15,
	left: 15,
	bottom:40
	
});

var prevBlock = Ti.UI.createView({
	backgroundColor: "303030",
	top:20,
	right:150,
	left: 15,
	height:100
	
});


currentWindow.add(imageBlock, prevBlock);


